{
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<c-c>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-w>",
                "h"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-w>",
                "l"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "prettier.requireConfig": true,
    "editor.formatOnSave": true,
    "workbench.fontAliasing": "auto",
    "git.autofetch": true,
    "remote.SSH.configFile": "~/.ssh/workspace",
    "remote.SSH.showLoginTerminal": true,
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "stylelint.validate": [
        "css",
        "html",
        "less",
        "markdown",
        "postcss",
        "sass",
        "scss",
        "source.css.styled",
        "source.markdown.math",
        "styled-css",
        "sugarss",
        "svelte",
        "vue",
        "vue-html",
        "vue-postcss",
        "xml",
        "xsl"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "[scss]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[handlebars]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "ruby.lint": {
        "rubocop": {
            "useBundler": true
        }
    },
    "ruby.format": "rubocop",
    "ruby.useLanguageServer": true,
    "[ruby]": {
        "editor.formatOnSave": true
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "entity.name.type.class",
                    "entity.other.attribute-name",
                    "keyword",
                    "constant",
                    "storage.modifier",
                    "storage.type.class.js"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.minimap.enabled": false,
    "js/ts.implicitProjectConfig.experimentalDecorators": true,
    "workbench.iconTheme": "file-icons",
    "editor.cursorStyle": "line",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "vim.smartRelativeLine": true,
    "editor.stickyTabStops": true,
    "terminal.integrated.fontFamily": "JetBrains Mono NL, Cascadia Code, Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.fontWeight": "600",
    "editor.fontLigatures": true,
    "editor.fontWeight": "normal",
    "editor.fontFamily": "CaskaydiaCove Nerd Font, Cascadia Code, JetBrains Mono NL, Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.suggestSelection": "first",
    "editor.lineNumbers": "relative",
    "editor.tabSize": 2,
    "editor.inlineSuggest.enabled": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "workbench.commandPalette.preserveInput": true,
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/tmp": true,
        "**/.git": true,
        "**/.gitignore": true,
        "**/*.log": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/bower_components/**": true,
        "**/tmp/**": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/tmp": true,
        "**/node_modules": true
    },
    "files.trimTrailingWhitespace": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": false
    },
    "terminal.integrated.minimumContrastRatio": 1,
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#dd0531"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#61dafb"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Svelte Orange",
            "value": "#ff3d00"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "files.trimFinalNewlines": true,
    "[glimmer-js]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.foldingStrategy": "indentation"
    },
    "[glimmer-ts]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.foldingStrategy": "indentation"
    },
    "eslint.validate": [
        "glimmer-ts",
        "glimmer-js"
    ],
    "eslint.rules.customizations": [
        {
            "rule": "*",
            "severity": "warn"
        }
    ],
    "prettier.documentSelectors": [
        "**/*.gts",
        "**/*.gjs"
    ],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "githubPullRequests.pullBranch": "never",
    "github.copilot.editor.enableAutoCompletions": true,
    "rubyLsp.rubyVersionManager": {
        "identifier": "auto"
    },
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "cursor.cpp.disabledLanguages": [
        "markdown"
    ],
    "workbench.colorTheme": "Catppuccin Frapp√©"
}